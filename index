UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    if input.KeyCode == Enum.KeyCode.E then
        game.Workspace.BankRobbery.VaultDoor.Door.Attachment.ProximityPrompt.HoldDuration = 0
        game.Workspace.BankRobbery.VaultDoor.Door.Attachment.ProximityPrompt.Enabled = true
    end
end)
UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    if input.KeyCode == Enum.KeyCode.W then
        while true do
        wait(0.01)
            if game:GetService("Players").LocalPlayer.PlayerGui:FindFirstChild("PurchaseDialog") then
                game:GetService("Players").LocalPlayer.PlayerGui.PurchaseDialog.Frame.CostLabel.Text = "ÂÖçË¥π"
            end
        end
        
    end
end)

UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    if input.KeyCode == Enum.KeyCode.Q then
        Game.Workspace.Game.Airdrops.Airdrop.Airdrop.ProximityPrompt.HoldDuration = 0
    end
end)
UserInputService.InputBegan:Connect(function(input, gameProcessedEvent)
    if input.KeyCode == Enum.KeyCode.C then
        
        local PlayerAABB = game:GetService("Players").LocalPlayer 
        if game.workspace:FindFirstChild("AABB") then
            game.workspace.AABB:Destroy()
            local charlieFoder = Instance.new("Folder")
            charlieFoder.Parent = game.workspace
            charlieFoder.Name = "AABB"
            local charliepart = Instance.new("Part")
            charliepart.Transparency = 1
            charliepart.Parent = game.workspace.AABB
            charliepart.Position = PlayerAABB.Character["LeftFoot"].Position
            charliepart.Anchored = true
        else
            local charlieFoder = Instance.new("Folder")
            charlieFoder.Parent = game.workspace
            charlieFoder.Name = "AABB"
            local charliepart = Instance.new("Part")
            charliepart.Transparency = 1
            charliepart.Parent = game.workspace.AABB
            charliepart.Position = PlayerAABB.Character["LeftFoot"].Position
            charliepart.Anchored = true
        end
    end
end)
